{
    "name": "dynamis/framework",
    "license": "MIT",
    "description": "Dynamis is a lightweight component based PHP framework that integrates a modern workflow, through Tekton, into Wordpress.",
    "homepage": "https://github.com/dynamis-wp/framework",
    "autoload": {
        "psr-4": {
            "Dynamis\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "composer/installers": "~1.0",
        "tekton/framework": "^2.0.0",
        "tekton/assets": "^2.0.0",
        "tekton/support": "^2.1.0",
        "tekton/session": "^2.0.0",
        "tekton/components": "^1.0.0",
        "illuminate/view": "~5.4.0",
        "illuminate/contracts": "~5.4.0",
        "nsrosenqvist/blade-compiler": "^1.3.0",
        "intervention/image": "^2.4.1",
        "rarst/wps": "^1.1"
    }
}
